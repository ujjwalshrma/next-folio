          "single": "paragraph,hyperlink,strong,em"
        }
      },
      "project_labels": {
        "type": "Group",
        "config": {
          "label": "Project Labels",
          "fields": {
            "label": {
              "type": "Select",
              "config": {
                "label": "label",
                "placeholder": "",
                "options": ["Role", "Technology", "Date"],
                "default_value": "Role"
              }
            },
            "label_text": {
              "type": "Text",
              "config": {
                "label": "Label Text",
                "placeholder": ""
              }
            }
          }
        }
      },
      "view_button_text": {
        "type": "Text",
        "config": {
          "label": "View Button Text",
          "placeholder": ""
        }
      },
      "view_button_link": {
        "type": "Link",
        "config": {
          "label": "View Button Link",
          "placeholder": "",
          "select": null
        }
      },
      "project_image": {
        "type": "Group",
        "config": {
          "label": "Project Image",
          "fields": {
            "images": {
              "type": "Image",
              "config": {
                "label": "Images",
                "constraint": {},
                "thumbnails": []
              }
            }
          }
        }
      }
    }
  }
