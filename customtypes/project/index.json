      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "main_project_image": {
        "type": "Image",
        "config": {
          "label": "Main Project Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "project_title": {
        "type": "Text",
        "config": {
          "label": "Project Title",
          "placeholder": ""
        }
      },
      "project_description": {
        "type": "StructuredText",
        "config": {
          "label": "Project Description",
          "placeholder": "",
